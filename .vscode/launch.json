{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch React App in Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}/src",
			"preLaunchTask": "start-dev-server"
		},
		{
			"name": "Attach to Chrome",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"url": "http://localhost:3000",
			"webRoot": "${workspaceFolder}/src"
		}
	]
}
